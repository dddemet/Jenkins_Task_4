#!/usr/bin/env groovy

pipeline {
	agent any
    parameters {
        disableConcurrentBuilds()
        timestamps()
    }
    stages {
        stage('BUILD') {
            steps{
                sh "TEST"
            }
        }
        stage('TEST') {
			steps {
                sh "TEST"
            }
	  	}
        stage('PUSH') {
            steps{
                sh "TEST"
            }
        }
        post {
            always {
                echo "TEST"
            }
        }
    }
}
