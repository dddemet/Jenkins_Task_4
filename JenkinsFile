#!/usr/bin/env groovy

pipeline {
	agent any
    stages {
        stage('BUILD') {
            steps{
                sh "docker build -t nginxlab ."
            }
        }
        stage('TEST') {
			steps {
                sh "echo TEST"
            }
	  	}
        stage('PUSH') {
            steps{
                sh "echo TEST"
            }
        }
    }
}
