#!/usr/bin/env groovy

pipeline {
	agent any
    parameters {
        disableConcurrentBuilds()
        timestamps()
    }
    stages {
        stage('build') {
            steps{
                sh "TEST"
            }
        }
        stage('test') {
			steps {
                sh "TEST"
            }
	  	}
        stage('push') {
            steps{
                sh "TEST"
            }
        }
    post {
        always {
            echo "TEST"
        }
    }
}
